<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"> 
<!-- 外部大矩形 -->
<item> 
      <shape> 
            <solid android:color="@color/business_bkg_grey" />
            <corners  android:topLeftRadius="6dp" android:bottomLeftRadius="6dp"/>
            <size android:width="56dp" android:height="35dp"/>
            <stroke android:width="0.5dp" android:color="@color/blue"/>
      </shape> 
</item>

<!-- 内部小矩形-->
<item   android:top="2dp" android:bottom="2dp" android:left="20dp"> 
     <shape> 
           <solid android:color="@color/business_bkg_grey"/> 
           <padding android:bottom="2dp"
           		android:left="2dp"
           		android:right="2dp"
           		android:top="2dp" />
           <size android:width="30dp" android:height="32dp"/>
     </shape>     
</item>
</layer-list>
