<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 连框颜色值 -->
    <item>
        <shape>
            <solid android:color="#828299" />
        </shape>
    </item>
    <!-- 主体背景颜色值 -->
    <item
        android:top="@dimen/size_1"
        android:bottom="@dimen/size_1"
        android:left="@dimen/size_1">
        <shape>
            <solid android:color="#E4E6E9" />
            <padding
                android:bottom="@dimen/size_1"
                android:left="@dimen/size_1"
                android:right="@dimen/size_1"
                android:top="@dimen/size_1" />
        </shape>
    </item>
</layer-list><!--
