<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="HJCustomCheckboxTheme" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/hjcheck_box</item>
    </style>

    <style name="Theme.HjNoActionBar" parent="Theme.AppCompat.Light.NoActionBar"></style>

    <style name="Theme.Hjactionbar" parent="@style/Theme.AppCompat.Light.DarkActionBar">
        <item name="actionBarSize">@dimen/abc_action_bar_default_height</item>
        <item name="android:actionBarStyle">@style/MMActionBar</item>
        <item name="colorPrimary">#27a4e3</item>
        <item name="colorPrimaryDark">#27a4e3</item>
        <item name="android:actionBarDivider">@null</item>
        <item name="android:windowContentOverlay">@null</item>
        <!-- 鑷畾涔夌殑actionbar鍚戜笂鎸夐挳 -->
        <item name="android:popupMenuStyle">@style/MyPopupMenu</item>
    </style>

    <style name="MyPopupMenu" parent="android:style/Widget.Holo.Light.ListPopupWindow">
        <item name="android:popupBackground">@color/actionbar_bkg_stacked</item>
    </style>

    <style name="MMActionBar" parent="@style/Widget.AppCompat.ActionBar">
        <item name="android:actionBarItemBackground">@color/actionbar_bkg_stacked</item>
        <!-- <item name="android:icon">@drawable/actionbar_icon_layer_big_padding</item> -->
        <item name="android:background">@color/actionbar_bkg_stacked</item>
        <item name="titleTextStyle">@style/MMActionBar.TitleTextStyle</item>
        <item name="background">@color/actionbar_bkg_stacked</item>
        <item name="backgroundStacked">@color/actionbar_bkg_stacked</item>
        <item name="backgroundSplit">@color/actionbar_bkg_stacked</item>
        <item name="indeterminateProgressStyle">@style/ActionBarCustomProgressBar</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:displayOptions">useLogo|showHome</item>
        <item name="elevation">0dp</item>
    </style>

    <style name="ActionBarCustomProgressBar" parent="@style/Widget.AppCompat.ProgressBar"></style>

    <!-- ActionBar title text -->
    <!--
    <style name="MMActionBar.TitleTextStyle" parent="Base.TextAppearance.AppCompat.Title">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/abc_action_bar_title_text_size</item>
    </style>








    -->
    <style name="MMActionBar.TitleTextStyle" parent="@style/TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textSize">@dimen/ActionBarTextSize</item>
        <item name="android:textColor">@color/actionbar_bkg_black</item>
    </style>

    <style name="mystyle" parent="android:Animation">
        <item name="@android:windowEnterAnimation">@anim/head_in</item>
        <!-- 杩涘叆鏃剁殑鍔ㄧ敾 -->
        <item name="@android:windowExitAnimation">@anim/head_out</item>
        <!-- 閫�鍑烘椂鐨勫姩鐢� -->
    </style>

    <style name="leftttomstyle" parent="android:Animation">
        <item name="@android:windowEnterAnimation">@anim/head_in</item>
        <!-- //杩涘叆鏃剁殑鍔ㄧ敾 -->
        <item name="@android:windowExitAnimation">@anim/head_out</item>
        <!-- //閫�鍑烘椂鐨勫姩鐢� -->
    </style>

    <style name="showPicDialogStyle" parent="android:Theme.Dialog">
        <item name="android:windowBackground">@color/black</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowContentOverlay">@color/black</item>
        <!-- 瀵硅瘽妗嗘槸鍚︽湁閬洊 -->
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="noticeDialogStyle" parent="android:Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <!-- 瀵硅瘽妗嗘槸鍚︽湁閬洊 -->
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="input_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:windowFrame">@null</item>
        <!-- Dialog鐨剋indowFrame妗嗕负鏃� -->
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/transparent</item>
    </style>

    <style name="EditText" parent="@android:style/Widget.EditText">
        <item name="android:background">@drawable/edittext_bg_selector</item>
    </style>

    <style name="SelectPopupWindowAnimation" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/select_popup_window_enter</item>
        <item name="android:windowExitAnimation">@anim/select_popup_window_exit</item>
    </style>

    <style name="top_but_left">
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">16sp</item>
        <item name="android:background">@drawable/qq_constact_group</item>
    </style>

    <style name="top_but_right">
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">16sp</item>
        <item name="android:background">@drawable/qq_constact_all</item>
    </style>

    <style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <!-- 鏄惁娴幇鍦╝ctivity涔嬩笂 -->
        <item name="android:windowIsTranslucent">false</item>
        <!-- 鍗婇�忔槑 -->
        <item name="android:windowNoTitle">true</item>
        <!-- <item name="android:windowBackground">@drawable/cc</item> -->
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowBackground">@color/transparent</item>
        <!-- 鑳屾櫙閫忔槑 -->
        <item name="android:backgroundDimAmount">1</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Translucent</item>
    </style>

    <style name="discard_radio_button_style">
        <item name="android:button">@null</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_height">35dp</item>
        <!-- <item name="android:textColor">@drawable/rb_text_selector</item> -->
        <!-- <item name="android:background">@drawable/background_selector</item> -->
    </style>

    <style name="OverflowMenu" parent="Widget.AppCompat.PopupMenu.Overflow">

        <!-- 鍏煎Api 21涔嬪墠鐨勭増鏈� -->
        <item name="overlapAnchor">false</item>
    </style>

    <style name="HJMapAppTheme" parent="Theme.Hjactionbar">

        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
        <item name="actionOverflowMenuStyle">@style/OverflowMenu</item>
        <item name="android:actionBarWidgetTheme">@style/MyActionWidgetTheme</item>
        <item name="actionBarWidgetTheme">@style/MyActionWidgetTheme</item>
        <!--
        <item name="android:background">#27a4e3</item>
        <item name="android:textColor">@android:color/white</item>



        -->

    </style>

    <style name="Theme.MAPToolBar.Base" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="actionMenuTextColor">@color/yellow</item>
    </style>

    <style name="MyActionWidgetTheme" parent="@style/Widget.AppCompat.Toolbar">
        <item name="android:textColor">#f00</item>
    </style>



    <style name="menu_item_text_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:background">@drawable/menu_item_selector</item>
    </style>

</resources>